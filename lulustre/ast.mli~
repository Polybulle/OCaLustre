type 'a node = {
 name : string ;
 inputs : string list;  
 outputs : string list; 
 equations : 'a equation list; 
}
and 
 'a equation = { 
 pattern : 'a pattern ; 
 expression : 'a expression; 
} 
and 
 'a pattern = ('a ident) list
and
 camlvalue 
and
  ('a, 'b) loc = { loc : 'a ; content : 'b }
and
 'a expression = ('a , 'a exp_desc) loc 
and
 'a ident = ('a , string) loc  
and
 'a exp_desc =   
  | Alternative of 'a expression * 'a expression * 'a expression 
  | InfixOp of inf_operator * 'a expression * 'a expression 
  | PrefixOp of pre_operator * 'a expression
  | Value of camlvalue  
  | Variable of 'a ident  
and
 inf_operator = 
  | Plus
  | Minus
  | Times
  | Div
  | Arrow 
and
 pre_operator = 
  | Not
  | Opposed 
  | Pre 
